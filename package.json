{
  "name": "opal-webpack",
  "version": "0.10.0-beta2",
  "description": "Opal Ruby module loader for webpack",
  "main": "index.js",
  "scripts": {
    "start": "babel --watch index.es6.js --out-file index.js",
    "build_compiler": "bundle exec opal --no-exit -c compiler.rb | uglifyjs > vendor/opal-compiler.js",
    "test": "mocha",
    "prepare": "npm run build_compiler; babel index.es6.js --out-file index.js; npm test",
    "prepublish": "npm run prepare"
  },
  "author": "CJ Lazell",
  "contributors": [{
    "name": "David Chang"
  }],
  "keywords": [
    "webpack",
    "loader",
    "opal",
    "ruby",
    "transpiler",
    "module"
  ],
  "license": "MIT",
  "dependencies": {
    "bundle-loader": "^0.5.4",
    "exports-loader": "^0.6.2",
    "glob": "^5.0.14",
    "imports-loader": "^0.6.4",
    "loader-utils": "^0.2.11",
    "source-map": "^0.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.6.0",
    "uglifyjs": "^2.4.1"
  }
}
